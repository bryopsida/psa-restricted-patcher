{
  "tls": {
    "enabled": false,
    "keyPath": "/var/run/secrets/tls/tls.key",
    "certPath": "/var/run/secrets/tls/tls.crt"
  },
  "log": {
    "level": "info"
  },
  "addSeccompProfile": true,
  "seccompProfile": "RuntimeDefault",
  "namespaces": [],
  "defaultGid": 1001,
  "defaultUid": 1001,
  "defaultFsGroup": 1001,
  "passthrough": ["^test.*$"]
}
